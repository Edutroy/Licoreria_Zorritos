#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/coffee" ]; then
  exec "$basedir/coffee"  "$basedir/../issue/app/cli/bin/issue" "$@"
else 
  exec coffee  "$basedir/../issue/app/cli/bin/issue" "$@"
fi
